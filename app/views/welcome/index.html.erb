<h1>Viewing Party!</h1>

<div id="login">
  <%= button_to "Create a New User", "/register", method: :get %>
  <% if current_user %>
    <%= button_to "Log Out", logout_path, method: :delete %>
  <% else %>
    <%= button_to "Log In", login_path, method: :get  %>
  <% end %>
</div>


<ul id="existing_users">
  <h3>Existing Users</h3>
    <% @users.each do |user| %>
      <li><%= link_to "#{user.email}", dashboard_path(user) %></li>
    <% end %>
</ul>
